package stationBL;

import javax.xml.bind.annotation.XmlAttribute;

public class WantsFuel {
	@XmlAttribute
	public float numOfLiters;
	@XmlAttribute
	public int pumpNum;
	@Override
	public String toString() {
		return "WantsFuel [numOfLiters=" + numOfLiters + ", pumpNum="
				+ pumpNum + "]";
	}
}
