package stationBL;

import javax.xml.bind.annotation.XmlAttribute;
import javax.xml.bind.annotation.XmlRootElement;
@XmlRootElement
public class Car {
	@XmlAttribute
	public boolean wantCleaning;
	@XmlAttribute
	public int id;
	@Override
	public String toString() {
		return "Car [wantCleaning=" + wantCleaning + ", id=" + id + "]";
	}
	
	class InnerClass {
        ...
    }
    
}
